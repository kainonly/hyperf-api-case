{
  "name": "laravel/lumen",
  "description": "Van component restful api by Lumen",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "barryvdh/laravel-cors": "^0.11.3",
    "illuminate/cookie": "^5.8",
    "laravel/lumen-framework": "5.8.*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
